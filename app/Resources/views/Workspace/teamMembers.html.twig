{% if ws.teamMember|length != "0" %}
  <h2>L'équipe à votre service</h2>

  {% for teamMember in ws.teamMember %}            
    <div class="members">
      <img src="{{ asset('images/Workspaces/') }}{{ws.name}}/{{teamMember.urlProfilePicture}}" alt="{{teamMember.urlProfilePicture}}" width="150px" height="150px"><br>
      {{teamMember.firstname}} {{teamMember.name}}
      <div class="label label-info">{{teamMember.job}}</div>
    </div>
  {% endfor %}

{% else %}
  {% for p in pictures %}
    {% if p.isFavorite %}
      <img src="{{ asset('images/Workspaces/') }}{{ws.name}}/{{p.url}}" alt="{{ws.name}}" width="100%">
    {% endif %}
  {% endfor %}
{% endif %}
